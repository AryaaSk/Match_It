{
    "compilerOptions": {
        "target": "ESNext",  
        "module": "ESNext",
        "moduleResolution": "Node",
        "outDir": ".JS",   
        "esModuleInterop": true,   
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,  
        "strict": true,   
        "skipLibCheck": true,
    }
}